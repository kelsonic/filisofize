<div class="row">
  <divclass="col s12 center teal-text">
    <h3>Welcome Filosofer...</h3>
  </div>
</div>

<div class="row">
  <form class="col s12" action="/filosofers" method='post'>

    <div class="row">
      <div class="input-field col s4 teal-text">
        <input placeholder="Socates" id="filosofer[first_name]" type="text" class="validate">
        <label for="first_name">First Name</label>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s4 teal-text">
        <input placeholder="Plato" id="filosofer[last_name]" type="text" class="validate">
        <label for="last_name">Last Name</label>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s10 teal-text">
        <input id="email" type="filosofer[email]" class="validate">
        <div class="row">
        <label for="email" data-error="please enter a valid email address" data-success="Thanks">Email</label>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s10 teal-text">
        <input placeholder="therfore-i-am" id="filosofer[username]" type="text" class="validate">
        <div class="row">
        <label for="username">Filosofer Username</label>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="input-field col s10 teal-text">
        <input id="password" type="filosofer[password]" class="validate">
        <div class="row">
        <label for="password">Password</label>
        </div>
      </div>
    </div>

    <div class = "row">
      <button class="btn waves-effect waves-light" type="submit" name="action">Submit
        <i class="material-icons right">send</i>
      </button>
    </div>

  </form>
</div>
